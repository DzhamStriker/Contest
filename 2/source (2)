#include <iostream>
#include <vector>

using std::cin;
using std::cout;
using std::endl;

char CaseChange(char c) {
    if (static_cast<char> (c) >= 97 && static_cast<char> (c) <= 122) {
        return (static_cast<char>(static_cast<char> (c) - 32));
    } else if (static_cast<char> (c) >= 65 && static_cast<char> (c) <= 90) {
        return (static_cast<char>(static_cast<char> (c) + 32));
    } else {
        return c;
    }
}