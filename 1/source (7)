#include <iostream>
#include <string>

using std::cin;
using std::cout;
using std::string;

int main() {
    string words;
    getline(cin, words);
    int result = 1;
    for (int i = 0; i < words.length(); i++) {
        if (words[i] == ' ')
            result++;
    }
    cout << result;
}