#include <iostream>

using std::cin;
using std::cout;

int main() {
    int x1, y1, x2, y2;
    cin >> x1;
    cin >> y1;
    cin >> x2;
    cin >> y2;
    if (x1 == x2 || y1 == y2 || x1 - y1 == x2 - y2 || x1 + y1 == x2 + y2) {
        cout << "YES";
    } else {
        cout << "NO";
    }
}