#include <iostream>
#include <string>
#include <stack>

using std::cout;
using std::cin;
using std::string;

int main() {
    std::stack<int> numbers;
    int number;
    string call;
    while (cin >> call) {
        if (call == "push") {
            cin >> number;
            numbers.push(number);
            cout << "ok" << '\n';
        } else if (call == "pop") {
            if (numbers.size() > 0) {
                cout << numbers.top() << '\n';
                numbers.pop();
            } else {
                cout << "error" << '\n';
            }
        } else if (call == "back") {
            if (numbers.size() > 0) {
                cout << numbers.top() << '\n';
            } else {
                cout << "error" << '\n';
            }
        } else if (call == "size") {
            cout << numbers.size() << '\n';
        } else if (call == "clear") {
            numbers = std::stack<int>();
            cout << "ok" << '\n';
        } else if (call == "exit") {
            cout << "bye" << '\n';
            break;
        }
    }
}